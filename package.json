{
  "name": "distribution-chart",
  "scripts": {
    "start": "npm run server & npm run build",
    "server": "http-server . -p 3000 -s",
    "build": "NODE_PATH=node_modules:. watchify -e index.web.js -t babelify -o bundle.js -dv",
    "bundle": "mkdir -p dist && cp index.html dist/index.html && NODE_PATH=node_modules:. browserify -e index.web.js -t babelify -o dist/bundle.js"
  },
  "dependencies": {
    "d3-array": "~0.7.1",
    "d3-format": "~0.5.1",
    "d3-interpolate": "~0.7.0",
    "d3-random": "~0.2.1",
    "d3-scale": "~0.6.4",
    "d3-shape": "~0.6.0",
    "flux": "~2.1.1",
    "immutable": "~3.7.6",
    "invariant": "~2.2.1",
    "react": "~0.14.7",
    "react-dom": "~0.14.7",
    "react-motion": "~0.4.2"
  },
  "devDependencies": {
    "babel-core": "~6.7.0",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-react": "~6.5.0",
    "babelify": "~7.2.0",
    "browserify": "~13.0.0",
    "http-server": "~0.9.0",
    "watchify": "~3.7.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
